  static String _statusToString(LeadStatus status) {
    switch (status) {
      case LeadStatus.new_:
        return 'new';
      case LeadStatus.called:
        return 'called';
      case LeadStatus.interested:
        return 'interested';
      case LeadStatus.converted:
        return 'converted';
      case LeadStatus.dnc:
        return 'dnc';
    }
  }
}
